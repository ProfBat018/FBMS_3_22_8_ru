<Window x:Class="DelegateButton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DelegateButton"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <StackPanel>
            <Button Command="{Binding ClickCommand}" Width="50" Height="50">Click me</Button>
            <Button Command="{Binding SecondClickCommand}" Width="50" Height="50">Click me2</Button>
        </StackPanel>
    </Grid>
</Window>